    <pb_type name="CARRY4" num_pb="1" blif_model=".subckt CARRY4">
     <input name="S"  num_pins="4"/>
     <input name="DI" num_pins="4"/>

     <output name="CO" num_pins="4"/>
     <output name="O"  num_pins="4"/>

     <input name="CIN"    num_pins="1"/>
     <input name="CYINIT" num_pins="1"/>

     <!-- CO output -->
     <delay_constant max="10e-12" in_port="CARRY4.S"      out_port="CARRY4.CO"/>
     <delay_constant max="10e-12" in_port="CARRY4.DI"     out_port="CARRY4.CO"/>
     <delay_constant max="10e-12" in_port="CARRY4.CIN"    out_port="CARRY4.CO"/>
     <delay_constant max="10e-12" in_port="CARRY4.CYINIT" out_port="CARRY4.CO"/>
     <!-- O output -->
     <delay_constant max="10e-12" in_port="CARRY4.S"      out_port="CARRY4.O"/>
     <delay_constant max="10e-12" in_port="CARRY4.DI"     out_port="CARRY4.O"/>
     <delay_constant max="10e-12" in_port="CARRY4.CIN"    out_port="CARRY4.O"/>
     <delay_constant max="10e-12" in_port="CARRY4.CYINIT" out_port="CARRY4.O"/>
    </pb_type>
